{
  "name": "restaura",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "test:load": "k6 run load-tests/api-load-test.js",
    "test:load:quick": "k6 run --vus 10 --duration 30s load-tests/api-load-test.js",
    "test:load:journey": "k6 run load-tests/customer-journey.js",
    "backup": "bash scripts/backup.sh",
    "seed:tenants": "node scripts/seed-tenants.js",
    "seed:superadmin": "node scripts/seed-superadmin.mjs",
    "verify:supabase": "node scripts/verify-supabase.js",
    "test:schema": "export $(cat .env.local | grep -v '^#' | xargs) && node scripts/test-schema-migration.mjs",
    "migrate:fix": "./scripts/apply-migration.sh",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "prepare": "husky"
  },
  "dependencies": {
    "@sentry/react": "^10.27.0",
    "@sentry/vite-plugin": "^4.6.1",
    "@supabase/supabase-js": "^2.74.0",
    "@tanstack/react-query": "^5.90.11",
    "canvas-confetti": "^1.9.4",
    "chart.js": "^4.5.1",
    "date-fns": "^4.1.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.23.24",
    "i18next": "^25.7.1",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
    "jszip": "^3.10.1",
    "lucide-react": "^0.545.0",
    "papaparse": "^5.5.3",
    "pg": "^8.16.3",
    "prop-types": "^15.8.1",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.6.0",
    "react-i18next": "^16.3.5",
    "react-icons": "^5.5.0",
    "react-qr-code": "^2.0.18",
    "react-router-dom": "^7.9.3",
    "recharts": "^3.3.0",
    "styled-components": "^6.1.19",
    "use-sync-external-store": "^1.6.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@eslint/js": "^9.21.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^4.0.14",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.3",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "happy-dom": "^20.0.10",
    "husky": "^9.1.7",
    "jsdom": "^24.1.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.3",
    "prettier": "^3.7.3",
    "tailwindcss": "^3.4.17",
    "vite": "^6.2.0",
    "vitest": "^4.0.14",
    "zod": "^4.1.13",
    "zustand": "^5.0.9"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ],
    "*.{json,css,md}": [
      "prettier --write --ignore-unknown"
    ]
  }
}
